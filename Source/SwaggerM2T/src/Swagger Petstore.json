{
    "swagger": "2.0",
    "info": {
        "title": "Swagger Petstore",
        "description": "Move your app forward with the Pet  API",
        "version": "1.0.0"
    },
    "host": "petstore.swagger.io",
    "schemes": [
        "http"
    ],
    "basePath": "/api",
	"consumes": [
		""
	],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/pets": {
            "get": {
				"summary": "Pets",
				"description": "Return every damn pet",
					 "parameters": [ 
						{
						"name": "limit",
						"in": "query",
						"description": "How many items to return",
						"required": false,
						"type": "integer"
					 }], 

				"tags": [
					"pets"
				],
				"responses": {
					"200": {
						"description": "A list of pets",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/Pet"
							}
						}
					}
				}
			}
		}, 
        "/exposicion": {
            "post": {
				"summary": "Exposervice",
				"description": "Servicio para la exposicion",
					 "parameters": [ 
						{
						"name": "test",
						"in": "query",
						"description": "query",
						"required": false,
						"type": "integer"
					},  

					
						{
						"name": "test2",
						"in": "query",
						"description": "Hola mundo",
						"required": true,
						"type": "integer"
					 }], 

				"tags": [
					"holamundo"
				],
				"responses": {
					"200": {
						"description": "Two",
						"schema": {
							"type": "string",
							"items": {
								"$ref": "#/definitions/Two"
							}
						}
					}
				}
			}
		}
    },
	"definitions": {
		"Pet": {
			"type": "object",
			"properties": {
					"id": {
						"type": "integer",
						"format": "int64" 
					}, 
					"name": {
						"type": "string" 
					}
			}

		}, 
		"Two": {
			"type": "string"
		}
	}
	
}	