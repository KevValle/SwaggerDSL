{
    "swagger": "2.0",
    "info": {
        "title": "Swagger Petstore",
        "description": "Move your app forward with the Pet  API",
        "version": "1.0.0"
    },
    "host": "petstore.swagger.io",
    "schemes": [
        "http"
    ],
    "basePath": "/api",
	"consumes": [
		""
	],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/pets": {
            "get": {
				"summary": "Pets",
				"description": "Return every pet",
					 "parameters": [ 
						{
						"name": "limit",
						"in": "query",
						"description": "How many items to return",
						"required": false,
						"type": "integer"
					 }], 

				"tags": [
					"pets"
				],
				"responses": {
					"200": {
						"description": "A list of pets",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/Pet"
							}
						}
					}, 
					"404": {
						"description": "Pet not found",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/Pet"
							}
						}
					}
				}
			}
		}, 
        "/petToOwner": {
            "put": {
				"summary": "Exposervice",
				"description": "Servicio para la exposicion",
					 "parameters": [ 
						{
						"name": "Ownerdata",
						"in": "query",
						"description": "Datos de la mascota",
						"required": true,
						"type": "integer"
					},  

					
						{
						"name": "PetData",
						"in": "query",
						"description": "Datos del dueño",
						"required": true,
						"type": "integer"
					 }], 

				"tags": [
					"Pets", 
					"owner", 
					"gestion"
				],
				"responses": {
					"200": {
						"description": "Invalid owner",
						"schema": {
							"type": "string",
							"items": {
								"$ref": "#/definitions/owner"
							}
						}
					}, 
					"300": {
						"description": "Invalid pet",
						"schema": {
							"type": "string",
							"items": {
								"$ref": "#/definitions/Pet"
							}
						}
					}
				}
			}
		}, 
        "/deletePet": {
            "delete": {
				"summary": "Deletepet",
				"description": "Delete pet",
					 "parameters": [ 
						{
						"name": "limit",
						"in": "query",
						"description": "How many items to return",
						"required": false,
						"type": "integer"
					 }], 

				"tags": [
					"pets"
				],
				"responses": {
					"200": {
						"description": "Pets deleted",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/Pet"
							}
						}
					}, 
					"404": {
						"description": "Pet not found",
						"schema": {
							"type": "array",
							"items": {
								"$ref": "#/definitions/Pet"
							}
						}
					}
				}
			}
		}
    },
	"definitions": {
		"Pet": {
			"type": "object",
			"properties": {
					"id": {
						"type": "integer",
						"format": "int64" 
					}, 
					"name": {
						"type": "string" 
					}, 
					"race": {
						"type": "string" 
					}
			}

		}, 
		"owner": {
			"type": "object",
			"properties": {
					"id": {
						"type": "integer",
						"format": "int64" 
					}, 
					"fullname": {
						"type": "string" 
					}, 
					"location": {
						"type": "string" 
					}
			}

		}
	}
	
}	